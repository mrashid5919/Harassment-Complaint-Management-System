<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: white;" styleClass="white-pane" stylesheets="@stylesheet.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Login.HomePage">
   <children>
      <VBox layoutX="209.0" prefHeight="112.0" prefWidth="598.0" style="-fx-background-color: #cc1436;">
         <children>
            <Button fx:id="logoutBut" mnemonicParsing="false" onAction="#logout" prefHeight="43.0" prefWidth="178.0" style="-fx-background-radius: 20; -fx-background-color: #FF767F;" text="Log out" translateX="420.0" translateY="65.0">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Button>
         </children>
      </VBox>
      <VBox prefHeight="600.0" prefWidth="212.0" style="-fx-background-color: #FF767F;">
         <children>
            <Label prefHeight="42.0" prefWidth="172.0" text="Options" translateX="35.0" translateY="40.0">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Label>
            <Button fx:id="DashBut" mnemonicParsing="false" onAction="#showDash" prefHeight="43.0" prefWidth="190.0" style="-fx-background-color: transparent; -fx-background-radius: 20;" text="DashBoard" translateY="120.0">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Button>
            <Button fx:id="complaintBut" mnemonicParsing="false" onAction="#showComplaint" prefHeight="43.0" prefWidth="190.0" style="-fx-background-color: transparent; -fx-background-radius: 20;" text="Add Complaint" translateY="140.0">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Button>
            <Button fx:id="historyBut" mnemonicParsing="false" onAction="#showHistory" prefHeight="43.0" prefWidth="190.0" style="-fx-background-color: transparent; -fx-background-radius: 20;" text="History" translateY="160.0">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Button>
            <Button fx:id="setBut" mnemonicParsing="false" onAction="#showSettings" prefHeight="43.0" prefWidth="190.0" style="-fx-background-color: transparent; -fx-background-radius: 20;" text="Settings " translateY="180.0">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Button>
            <Button fx:id="infoBut" mnemonicParsing="false" onAction="#showEditInfo" prefHeight="43.0" prefWidth="244.0" style="-fx-background-color: transparent; -fx-background-radius: 20;" text="Personal Info" translateY="200.0">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Button>
            <Button fx:id="pasChangeBut" mnemonicParsing="false" onAction="#changePass" prefHeight="43.0" prefWidth="206.0" style="-fx-background-color: transparent; -fx-background-radius: 20;" text="Password Change" translateY="210.0">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Button>
         </children>
      </VBox>
      <VBox fx:id="vbox" layoutX="241.0" layoutY="148.0" prefHeight="414.0" prefWidth="523.0" styleClass="white-pane" stylesheets="@stylesheet.css" />
   </children>
</AnchorPane>
